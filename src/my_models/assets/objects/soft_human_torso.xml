  </asset>
  <worldbody>
    <body>
        <body name="collision" pos="0.6 0 1" quat="0.5 0.5 -0.5 -0.5">
          <composite type="cylinder" count="10 5 12" spacing="0.06">
            <geom type="capsule" size=".015 0.05" rgba=".8 .2 .1 1"/>
            <skin material="skin-material"/>
          </composite>
        </body>
        <site rgba="0 0 0 0" size="0.005" pos="0 0 -0.12" name="bottom_site"/>
        <site rgba="0 0 0 0" size="0.005" pos="0 0 0.12" name="top_site"/>
        <site rgba="0 0 0 0" size="0.005" pos="0.5 0.5 0" name="horizontal_radius_site"/>
    </body>
  </worldbody>
</mujoco>