<mujoco model="tetrapack_gripper">
  <option solver="CG" tolerance="1e-6"/>
  <size nconmax="500" njmax="5000" nstack="50000"/>
  <asset>
    <texture file="../textures/milk.png" name="tex-milk" type="2d"/>
    <material name="milk" reflectance="0.5" texrepeat="1 1" texture="tex-milk" texuniform="true"/>  </asset>
  <worldbody>
    <!--For soft gripper-->
    <!-- <body name="gripper_base" pos="-0.004 -0.063 0.128" quat="0. 0. 0.707 0.707"> -->
    <!--For hard gripper-->
    <body name="gripper_base" pos="-0.004 -0.063 0.128" quat="0. 0. 0. 1">
      <geom name="box_collision" size="0.074 0.074 0.14" type="box" mass="0.01" friction="0.0001 0.005 0.0001" group="0"/>
      <geom name="box_visual" size="0.074 0.074 0.14" type="box" group="1"/>
      <!--For Soft object in container-->
      <!-- <composite prefix="EE" type="box" count="6 4 4" spacing="0.05" solrefsmooth="0.004 1">
        <skin texcoord="true" material="milk" rgba=".7 .7 .7 1"/>
        <geom type="capsule" size=".015 0.05" rgba=".8 .2 .1 1" mass="0.01" friction="0.01 0.005 0.0001" group="0"/>
      </composite> -->
      <!--For solid objects in container-->
      <!-- <composite prefix="EE" type="box" count="5 8 5" spacing="0.039" solrefsmooth="0.004 1">
        <geom type="capsule" size="0.0075 0.025" rgba=".8 .2 .1 1" mass="0.01" friction="0.01 0.005 0.0001" contype="3" group="0"/>
        <skin material="milk" texcoord="true"/>
      </composite> -->
      <site name="ft_frame" pos="0 0 0.135" size="0.074 0.074 0.01" rgba="1 0 0 1" type="box" group="1"/> 
      <site name="grip_site" pos="0 0 0.135" size="0.074 0.074 0.01" rgba="1 0 0 0.5" type="box" group="1"/>
      <site name="grip_site_cylinder" pos="0 0 0.135" size="0.074 0.074 0.01" rgba="0 1 0 0.3" type="box" group="1"/>
    </body>
  </worldbody>
  <sensor>
    <force name="force_ee" site="ft_frame"/>
    <torque name="torque_ee" site="ft_frame"/>
  </sensor>
</mujoco>